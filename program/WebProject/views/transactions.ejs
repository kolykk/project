  <div class="main">
    <div class="section section-nude">
      <div class="container">
        <div class="row">
          <div class="col-md-3">
            <div class="fixed-section">
              <ul>
               <a href="/register">
                 <div class="col-sm-3 block-3-box wow fadeInDown animated" style="visibility: visible; animation-name: fadeInDown;">
                  <br><br> <div class="block-3-box-icon">
                  <button class="btn btn-primary  btn-lg"><i class="fa fa-pencil" ></i>Registration</button>
                </a>
                <br><br> <a href="/manageinfo "> 
                <button class="btn btn-primary  btn-lg"><i class="fa fa-list-alt" ></i>Informations</button></a> 
                <br><br><a href="/transactions"> 
                <button class="btn btn-primary btn-lg"><i class="glyphicon glyphicon-transfer" ></i>Transactions</button></a>

              </ul>
            </div>
          </div>
          <div class="col-md-9">
            <div class="tim-container" style="min-height: 700px;">
              <div class="right_col" role="main">
                <div class="clearfix"></div>
                <div class="row">
                  <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                      <div class="x_title">

                        <h3><i class="glyphicon glyphicon-transfer" aria-hidden="true"></i> &nbsp TRANSACTIONS</h3>
                        <legend></legend>


                        <div class="clearfix"></div>
                      </div>
                      <div class="x_content">
                        <p class="text-muted font-13 m-b-30">
                          All Transactions send data to site.
                        </p>

                        <div class="box-body">
                         <div class="box-body">
                          <table id="example1" class="table table-bordered table-striped">


                            <thead>
                              <tr>
                                <th style="text-align:center;vertical-align:middle;">SENSOR ID</th>

                                <th>HUMIDITY VALUE</th>
                                <th>TEMPERATURE VALUE</th>
                                <th>LOCATION</th>
                                <th>CURRENT TIME</th>


                              </tr>
                            </thead>
                            <tbody style= "text-align:center;vertical-align:middle;">
                             <% for(var i=0; i<transactions.length; i++) { %>
                             <tr>
                               <td><%= transactions[i].s_id %></td>
                               <td><%= transactions[i].s_humidvalue %></td>
                               <td><%= transactions[i].s_tempvalue%></td>
                               <td><%= transactions[i].s_location%></td>
                               <td><%= transactions[i].currenttime %></td>


                             </tr>
                             <% } %>
                           </tbody>

                            <!-- alert

                            <td style='color:#FF0033'><span class="blink_text"><strong>ALERT!</strong></span></td> -->

                          </table>


                        </div>
                        <!-- Content Header (Page header) -->
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>






